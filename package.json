    "why-did-you-update": "^1.0.6"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.4.1",
    "nodemon": "^1.18.6"
  }
}
