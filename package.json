{
  "name": "iso",
  "version": "0.0.1",
  "description": "Iso is a framework for creating isomorphic single-page application with using SSR technology on NodeJS.",
  "main": "dist/server.js",
  "repository": "git@github.com:ogarich89/iso.git",
  "config": {
    "server": "--config config/webpack/server.config.babel.js",
    "client": "--config config/webpack/client.config.babel.js"
  },
  "scripts": {
    "client": "webpack -w $npm_package_config_client",
    "server": "BABEL_ENV=server webpack -w $npm_package_config_server",
    "nodemon": "gulp",
    "build": "webpack $npm_package_config_client && BABEL_ENV=server webpack $npm_package_config_server",
    "production": "NODE_ENV=production webpack $npm_package_config_client && NODE_ENV=production BABEL_ENV=server webpack $npm_package_config_server",
    "start": "webpack $npm_package_config_client && BABEL_ENV=server webpack $npm_package_config_server && node $npm_package_main",
    "test": "jest",
    "stats:client": "webpack --json > .stats/client.json $npm_package_config_client",
    "stats:server": "webpack --json > .stats/server.json $npm_package_config_server",
    "docgen": "react-docgen ./src/shared/components/ --pretty --out ./components.doc.json"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-import": {},
      "cssnano": {}
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "public/images"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass)$": "identity-obj-proxy"
    }
  },
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@loadable/babel-plugin": "^5.2.2",
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "@loadable/webpack-plugin": "^5.2.2",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "fbemitter": "^2.1.1",
    "glob": "^7.1.2",
    "i18next": "^15.0.4",
    "i18next-fetch-backend": "^2.2.0",
    "i18next-node-remote-backend": "^1.0.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "jest": "^24.1.0",
    "koa": "^2.6.1",
    "koa-bodyparser": "^4.2.1",
    "koa-ejs": "^4.1.2",
    "koa-generic-session": "^2.0.1",
    "koa-i18next": "^1.1.1",
