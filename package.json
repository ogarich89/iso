{
  "name": "iso",
  "version": "0.0.1",
  "description": "Iso is a framework for creating isomorphic single-page application with using SSR technology on NodeJS.",
  "main": "dist/server.js",
  "repository": "git@github.com:ogarich89/iso.git",
  "config": {
    "server": "--config config/webpack/server.config.babel.js",
    "client": "--config config/webpack/client.config.babel.js"
  },
  "scripts": {
    "client": "webpack -w $npm_package_config_client",
    "server": "BABEL_ENV=server webpack -w $npm_package_config_server",
    "nodemon": "gulp",
    "build": "webpack $npm_package_config_client && BABEL_ENV=server webpack $npm_package_config_server",
    "production": "NODE_ENV=production webpack $npm_package_config_client && NODE_ENV=production BABEL_ENV=server webpack $npm_package_config_server",
    "start": "yarn build && node $npm_package_main",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 years",
    "> 5%",
    "not IE 11"
  ],
  "pre-commit": [
    "test",
    "typecheck"
  ],
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
