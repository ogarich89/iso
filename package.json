{
  "name": "iso",
  "version": "0.0.1",
  "description": "Iso is a framework for creating isomorphic single-page application with using SSR technology on NodeJS.",
  "main": "dist/server.js",
  "repository": "git@github.com:ogarich89/iso.git",
  "config": {
    "server": "--config config/webpack/server.config.babel.js",
    "client": "--config config/webpack/client.config.babel.js"
  },
  "scripts": {
    "client": "webpack -w $npm_package_config_client",
    "server": "BABEL_ENV=server webpack -w $npm_package_config_server",
    "nodemon": "gulp",
    "build": "webpack $npm_package_config_client && BABEL_ENV=server webpack $npm_package_config_server",
    "production": "NODE_ENV=production webpack $npm_package_config_client && NODE_ENV=production BABEL_ENV=server webpack $npm_package_config_server",
    "start": "yarn build && node $npm_package_main",
    "test": "jest --coverage",
    "stats:client": "webpack --json > .stats/client.json $npm_package_config_client",
    "stats:server": "webpack --json > .stats/server.json $npm_package_config_server",
    "ts": "tsc",
    "ts:watch": "yarn ts -- --noEmit --watch",
    "docgen": "typedoc src/shared/components src/types --readme Readme.md --out docs --excludeExternals"
  },
  "browserslist": [
    "last 2 years",
    "> 5%",
    "not IE 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-tests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "public/images"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "pre-commit": [
    "test",
    "ts"
  ],
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
