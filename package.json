    "stable": "^0.1.8",
    "style-loader": "^0.23.0",
    "stylelint": "^9.7.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.25.1",
