{
  "name": "iso",
  "version": "2.0.0",
  "description": "Iso is a framework for creating isomorphic single-page application with using SSR technology on NodeJS.",
  "main": "server/index.mjs",
  "repository": "git@github.com:ogarich89/iso.git",
  "type": "module",
  "config": {
    "server": "--config config/rspack/server.config.mjs",
    "client": "--config config/rspack/client.config.mjs"
  },
  "engines": {
    "node": "^20.*"
  },
  "scripts": {
    "watch:client": "node run.mjs --watch:client",
    "watch:server": "node run.mjs --watch:server",
    "server": "node run.mjs --server",
    "dev": "node run.mjs --dev",
    "build": "rspack $npm_package_config_client && rspack $npm_package_config_server",
    "production": "NODE_ENV=production rspack $npm_package_config_client && NODE_ENV=production rspack $npm_package_config_server",
    "start": "yarn build && node $npm_package_main",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\" --customSyntax=postcss-scss"
  },
  "browserslist": [
    "last 2 years",
    "> 5%",
    "not IE 11"
  ],
  "pre-commit": [
    "test",
    "typecheck",
    "eslint",
    "stylelint"
  ],
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/session": "^11.0.1",
    "@fastify/static": "^8.0.1",
    "@fastify/view": "^10.0.1",
    "@loadable/component": "^5.16.4",
    "@loadable/server": "^5.16.5",
    "@reduxjs/toolkit": "^2.2.7",
    "@sentry/node": "^8.26.0",
    "axios": "^1.7.4",
    "classnames": "^2.2.6",
    "connect-redis": "^7.1.1",
    "core-js": "^3.38.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express-session": "^1.18.0",
    "fastify": "^5.0.0",
    "fbemitter": "^3.0.0",
    "i18next": "^23.13.0",
    "i18next-http-backend": "^2.5.2",
    "ioredis": "^5.4.1",
    "path-browserify": "^1.0.1",
    "pino-pretty": "^11.2.2",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "reset-css": "^5.0.1",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@loadable/webpack-plugin": "^5.2.2",
    "@prisma/client": "^5.18.0",
    "@rsdoctor/rspack-plugin": "^0.4.7",
    "@rspack/cli": "^1.0.14",
    "@rspack/core": "^1.0.14",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.7.11",
    "@swc/jest": "^0.2.36",
    "@swc/plugin-loadable-components": "^2.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/classnames": "^2.2.9",
    "@types/eslint__js": "^8.42.3",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^29.5.12",
    "@types/loadable__component": "^5.13.9",
    "@types/loadable__server": "^5.12.11",
    "@types/node": "^22.4.0",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/serialize-javascript": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.2",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.41",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-scss": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "react-refresh": "^0.14.2",
    "sass-embedded": "^1.77.8",
    "sass-loader": "^16.0.0",
    "stylelint": "^16.8.2",
    "swc-plugin-add-display-name": "^0.5.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "webpack-dev-middleware": "^7.3.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
